<!DOCTYPE html>
<head>
  {{> head}}
</head>
<body>
  {{>header}}
  {{> start}}
    <!-- my charts -->
    <div  id="chart" >
      <div id="event1">
      </div>
    </div>
    {{> end}}
    <!-- my js & css -->
    <link src="chart.css"></link>
    <script src="chart.js"></script>
    <style>
      .bar {
        fill: steelblue;
      }

      .axis text {
        font: 10px sans-serif;
      }

      .axis path,
      .axis line {
        fill: none;
        stroke: #000;
        shape-rendering: crispEdges;
      }

      .x.axis path {
        display: none;
      }
    </style>
  {{> footer}}
</body>
